{
    "terminal.integrated.cwd": "${workspaceFolder}",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.env.osx": {
        "DBT_PROJECT_DIR": "${workspaceFolder}",
        "PYTHONPATH": "${workspaceFolder}"
    },
    "terminal.integrated.env.linux": {
        "DBT_PROJECT_DIR": "${workspaceFolder}",
        "PYTHONPATH": "${workspaceFolder}"
    },
    "terminal.integrated.env.windows": {
        "DBT_PROJECT_DIR": "${workspaceFolder}"
    },
    "python.venvFolders": [
        "${workspaceFolder}/.venv"
    ],
    "python.envFile": "${workspaceFolder}/.env",
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.terminal.activateEnvironment": true,
    "terminal.integrated.inheritEnv": true,
    "dbt.lineage.showNonSelectEdges": true,
    "dbt.lineage.defaultExpansion": 9,
    "files.associations": {
        "/**/.github/**/*.yml": "github-actions-workflow"
    },
    "terminal.integrated.environmentChangesRelaunch": true,
    "dbt.dbtPythonPathOverride": "${workspaceFolder}/.venv/bin/python",
    "dbt.perspectiveTheme": "Monokai",
    "[jinja-sql]": {
        "editor.defaultFormatter": "henriblancke.vscode-dbt-formatter",
    },
    "editor.maxTokenizationLineLength": 20000,
    "dbt.buildModelCommandAdditionalParams": [
        "--no-print"
    ],
    "dbt.runModelCommandAdditionalParams": [
        "--no-print"
    ]
}